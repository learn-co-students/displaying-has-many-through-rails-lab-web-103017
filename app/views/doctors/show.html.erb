<h1><%=@doctor.name%> <%=@doctor.department%></h1>
<ul><%@doctor.appointments.each do |appointment| %>
  <li><%= link_to appointment.patient.name, patient_path(appointment.patient)%> | <%= link_to appointment.appointment_datetime.strftime("%B %d, %Y at %H:%M"), patient_path(appointment.patient)%></li>
<%end%>
</ul>
